{
  "name": "quezap",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "lint": "npm-run-all -s lint:ts lint:css",
    "lint:fix": "npm-run-all -s lint:ts:fix lint:css:fix",
    "lint:ts": "ng lint",
    "lint:ts:ci": "ng lint --format json --silent --quiet",
    "lint:ts:fix": "ng lint --fix",
    "lint:css": "node ./node_modules/.bin/stylelint --config './.stylelintrc' --max-warnings 0 \"src/**/*.css\"",
    "lint:css:ci": "node ./node_modules/.bin/stylelint --config './.stylelintrc' --max-warnings 0 \"src/**/*.css\" -f json",
    "lint:css:fix": "node ./node_modules/.bin/stylelint --config './.stylelintrc' --max-warnings 0 \"src/**/*.css\" --fix",
    "prepare": "husky"
  },
  "imports": {
    "#tailwind.css": "./src/styles/tailwind.css"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.0-rc.1",
    "@angular/compiler": "^21.0.0-rc.1",
    "@angular/core": "^21.0.0-rc.1",
    "@angular/forms": "^21.0.0-rc.1",
    "@angular/platform-browser": "^21.0.0-rc.1",
    "@angular/router": "^21.0.0-rc.1",
    "@ngrx/signals": "^20.1.0",
    "@primeuix/themes": "^1.2.5",
    "ngx-markdown": "^20.1.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "rxjs": "~7.8.2",
    "tailwindcss": "^4.1.17",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-eslint/builder": "20.5.1",
    "@angular/build": "^21.0.0-rc.1",
    "@angular/cli": "^21.0.0-rc.1",
    "@angular/compiler-cli": "^21.0.0-rc.1",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jest": "^30.0.0",
    "angular-eslint": "20.5.1",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jasmine-core": "~5.9.0",
    "jest": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "stylelint": "^16.25.0",
    "stylelint-actions-formatters": "^16.3.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.46.2"
  }
}
